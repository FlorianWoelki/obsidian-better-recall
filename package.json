{
  "name": "obsidian-better-recall",
  "version": "0.0.3",
  "description": "Add anki-like spaced repetition and recall to your vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src",
    "release": "mkdir -p dist && mv main.js dist/ && cp src/styles.css dist/ && cp manifest.json dist/ && pnpm version",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianWoelki/obsidian-better-recall.git"
  },
  "author": "Florian Woelki",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitest/coverage-istanbul": "^4.0.2",
    "builtin-modules": "5.0.0",
    "esbuild": "0.27.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.10",
    "obsidian": "1.10.3",
    "prettier": "^3.6.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "ts-fsrs": "^5.2.3",
    "uuid": "^13.0.0"
  }
}
