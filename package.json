{
  "name": "obsidian-better-recall",
  "version": "0.0.2",
  "description": "Add anki-like spaced repetition and recall to your vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint src",
    "release": "mkdir -p dist && mv main.js dist/ && cp src/styles.css dist/ && cp manifest.json dist/ && pnpm version",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianWoelki/obsidian-better-recall.git"
  },
  "author": "Florian Woelki",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "8.23.0",
    "@typescript-eslint/parser": "8.23.0",
    "@vitest/coverage-istanbul": "^3.0.5",
    "builtin-modules": "3.3.0",
    "esbuild": "0.14.47",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "happy-dom": "^17.0.2",
    "obsidian": "1.7.2",
    "prettier": "^3.5.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "uuid": "^10.0.0"
  }
}
